<page:pluto-portal-driver xmlns:page="http://liferay.com/pluto/xsd/pluto-portal-driver-config.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://liferay.com/pluto/xsd/pluto-portal-driver-config.xsd ../pluto-portal-driver-config.xsd ">
  <page:portal-name>pluto-portal-driver</page:portal-name>
  <page:portal-version>token</page:portal-version>
  <page:container-name>token</page:container-name>
  <page:supports>
    <page:portlet-mode>token</page:portlet-mode>
    <page:window-state>token</page:window-state>
  </page:supports>
  <page:portlet-app>
    <page:context-path>/dummypath</page:context-path>
